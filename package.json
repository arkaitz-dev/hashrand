{
	"name": "hashrand-spin-web",
	"private": true,
	"version": "0.4.0",
	"description": "Professional web interface for HashRand Spin API - hash, password, and API key generator",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.0",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.9"
	},
	"keywords": [
		"hash-generator",
		"password-generator",
		"api-key-generator",
		"svelte",
		"sveltekit",
		"typescript",
		"tailwindcss",
		"spa",
		"frontend",
		"web-interface"
	],
	"author": "HashRand Spin",
	"license": "MIT"
}